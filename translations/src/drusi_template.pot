msgid "Halbern"
msgstr ""

#. [part]
#: ./scenarios/04-watery-tunnels.cfg:154
msgid "The elves ventured underground once more, following the orcs' trail."
msgstr ""

#. [event]
#: ./scenarios/04-watery-tunnels.cfg:199
msgid "This looks like an interesting necklace."
msgstr ""

#. [event]
#: ./scenarios/04-watery-tunnels.cfg:199
msgid "Leave it alone"
msgstr ""

#. [event]
